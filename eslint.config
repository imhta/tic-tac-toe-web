{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": [
      "airbnb-base"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
    "arrow-body-style": "off",
    "arrow-parens": [2,"always"],
    "consistent-return": "off",
    "import/no-cycle": "off",
    "import/extensions":[ 1,"ignorePackages"]
  }
}